# Database (Supabase)
SUPABASE_DATABASE_URL=postgresql://postgres:postgres@localhost:5432/waygate

# Optional explicit issuer URL. If omitted, issuer is derived from request host + /a/{tenant}
# e.g. https://id.example.com/a/{tenant}
ISSUER_URL=

# SMTP (TBD values)
SMTP_HOST=
SMTP_PORT=
SMTP_USER=
SMTP_PASS=
EMAIL_FROM=

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_USERNAME=
REDIS_PASSWORD=

# Crypto
# Must be at least 32 characters for production
ENCRYPTION_KEY=dev_encryption_key_change_me_please_32_chars
SESSION_SECRET=dev_session_secret_change_me_please_32_chars
